{
  "name": "cortexone",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "concurrently -k \"vite\" \"npm:start-electron\"",
    "vite": "vite",
    "start-electron": "vite build -c vite.config.electron.ts && wait-on http://localhost:8547 && cross-env NODE_ENV=development electron . --no-sandbox",
    "build": "vite build && vite build -c vite.config.electron.ts && electron-builder",
    "preview": "vite preview",
    "electron:dev": "cross-env NODE_ENV=development electron . --no-sandbox"
  },
  "dependencies": {
    "@google/genai": "^1.22.0",
    "electron-store": "^11.0.2",
    "mic": "^2.1.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "three": "^0.168.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.14",
    "@types/node": "^22.14.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^38.2.1",
    "electron-builder": "^26.0.12",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.14",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "wait-on": "^9.0.1"
  }
}
