{
    "builds": [
      { "src": "server.js", "use": "@vercel/node" }
    ],
    "rewrites": [
      { "source": "/api/(.*)", "destination": "/server.js" },
      { "source": "/daily", "destination": "/daily.html" },
      { "source": "/podcast", "destination": "/podcast.html" },
      { "source": "/c", "destination": "/search.html" }
    ]
  }